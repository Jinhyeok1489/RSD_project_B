        </visual>

        <collision>
            <geometry>
                <box size="0.4 0.6 0.02"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0.2 0"/>
        </collision>

        <inertial>
            <mass value="100000"/>
