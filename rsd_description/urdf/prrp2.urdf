<robot name="prrp_robot">



    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.4 0.6 0.02"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0.2 0"/>
            <material name="grey2">
                <color rgba="0.9 0.9 0.9 1"/>
